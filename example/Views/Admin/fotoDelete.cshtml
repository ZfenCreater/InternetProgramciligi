@using example.Models
@model foto



@using (Html.BeginForm("fotoDelete", "Admin", FormMethod.Post, new { onsubmit = "submitForm(event, this)" }))
{
    <h2>@Model.imageurl adlı Fotoğrafı silmek istiyor musunuz?</h2>
    <div class="row mb-3">
        <div class="offset-sm-2 col-sm-10">
            <a onclick="hideModal()" class="btn btn-danger">İptal</a>
            <button type="submit" class="btn btn-primary">Kaydet</button>
        </div>
    </div>
}


